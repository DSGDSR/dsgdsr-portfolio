{
  "name": "portfolio",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "test:prod": "ng test --browsers=ChromeHeadless --watch=false --code-coverage",
    "env-config": "ts-node -O '{\"module\": \"commonjs\"}' ./env.config.ts",
    "build:browser": "ng build",
    "build:browser:prod": "npm run env-config && ng build --configuration production --aot",
    "deploy": "env-cmd node deploy.js",
    "deploy:prod": "node deploy.js",
    "lint": "ng lint",
    "gzip": "rm -rf ./dist/*-gzip && gzipper c --verbose --output-file-format [filename].[ext] ./dist/portfolio ./dist/portfolio-gzip",
    "dev:ssr": "ng run portfolio:serve-ssr",
    "dev:ssr:mac": "NODE_OPTIONS=--openssl-legacy-provider ng run portfolio:serve-ssr",
    "serve:ssr": "node dist/portfolio/server/main.js",
    "build:ssr": "ng build --configuration production && ng run portfolio:server:production",
    "prerender": "ng run portfolio:prerender",
    "vercel-build": "npm run build:ssr"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.4.0",
    "@angular/common": "^13.4.0",
    "@angular/compiler": "^13.4.0",
    "@angular/core": "^13.4.0",
    "@angular/platform-browser": "^13.4.0",
    "@angular/platform-browser-dynamic": "^13.4.0",
    "@angular/platform-server": "^13.4.0",
    "@angular/router": "^13.4.0",
    "@nguniversal/express-engine": "^13.4.0",
    "angular-feather": "6.1.0",
    "express": "^4.15.2",
    "localstorage-polyfill": "^1.0.1",
    "ngx-markdown": "~11.1.0",
    "prism-themes": "^1.9.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.11",
    "@angular/cli": "^13.3.11",
    "@angular/compiler-cli": "^13.4.0",
    "@nguniversal/builders": "^13.4.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.6.0",
    "@types/node": "~15.0.2",
    "codelyzer": "^6.0.0",
    "env-cmd": "^10.1.0",
    "gzipper": "^6.2.1",
    "jasmine-core": "~3.7.1",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.3.19",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.6.0",
    "sftp-upload": "^1.0.3",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.6.4"
  }
}